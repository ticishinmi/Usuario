program Usuario;

uses
  Vcl.Forms,
  UDM in 'UDM.pas' {DM: TDataModule},
  UPrinc in 'UPrinc.pas' {FPrinc},
  UAuditoria in 'UAuditoria.pas' {FAuditoria},
  UPesquisaIBO in 'UPesquisaIBO.pas' {FPesquisaIBO},
  UConfGrade in 'UConfGrade.pas' {FConfGrade},
  UCadPadrao in 'UCadPadrao.pas' {FPadrao};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TDM, DM);
  Application.CreateForm(TFPrinc, FPrinc);
  Application.CreateForm(TFAuditoria, FAuditoria);
  Application.CreateForm(TFPesquisaIBO, FPesquisaIBO);
  Application.CreateForm(TFConfGrade, FConfGrade);
  Application.CreateForm(TFPadrao, FPadrao);
  Application.Run;
end.
